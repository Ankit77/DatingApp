<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="SwipeFlingStyle" format="reference" />
        <attr name="rotation_degrees" format="float" />
        <attr name="min_adapter_stack" format="integer" />
        <attr name="max_visible" format="integer" />
    </declare-styleable>
    <declare-styleable name="ChipView">
        <attr name="chip_spacing" format="dimension" />
        <attr name="chip_line_spacing" format="dimension" />
        <attr name="chip_padding" format="dimension" />
        <attr name="chip_side_padding" format="dimension" />
        <attr name="chip_corner_radius" format="dimension" />
        <attr name="chip_background" format="reference" />
        <attr name="chip_background_selected" format="reference" />
        <attr name="chip_background_res" format="reference" />
        <attr name="chip_textcolor" format="reference" />
    </declare-styleable>

    <declare-styleable name="PullToZoomScrollView">
        <attr name="scrollHeadView" format="reference" />
        <attr name="scrollContentView" format="reference" />
        <attr name="scrollZoomView" format="reference" />
    </declare-styleable>
    <declare-styleable name="PullToZoomListView">
        <attr name="listHeadView" format="reference" />
        <attr name="isHeadParallax" format="boolean" />
    </declare-styleable>
    <declare-styleable name="PullToZoomView">
        <attr name="headerView" format="reference" />
        <attr name="contentView" format="reference" />
        <attr name="zoomView" format="reference" />
        <attr name="isHeaderParallax" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Pulsator4Droid">
        <attr name="pulse_count" format="integer" />
        <attr name="pulse_duration" format="integer" />
        <attr name="pulse_color" format="color" />
        <attr name="pulse_repeat" format="integer" />
        <attr name="pulse_maxScale" format="float" />
        <attr name="pulse_startFromScratch" format="boolean" />
        <attr name="pulse_interpolator" format="enum">
            <enum name="Linear" value="0" />
            <enum name="Accelerate" value="1" />
            <enum name="Decelerate" value="2" />
            <enum name="AccelerateDecelerate" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundCornerIndicator">
        <!-- width of indicators -->
        <attr name="rci_width" format="dimension" />
        <!-- height of indicators -->
        <attr name="rci_height" format="dimension" />
        <!-- the spacing between the indicator. -->
        <attr name="rci_gap" format="dimension" />
        <!-- cornerRadius of the indicator. -->
        <attr name="rci_cornerRadius" format="dimension" />
        <!-- width of the stroke used to draw the indicators. -->
        <attr name="rci_strokeWidth" format="dimension" />
        <!-- color of the indicator that represents the current page. -->
        <attr name="rci_selectColor" format="color" />
        <!-- color of the indicator that represents pages. -->
        <attr name="rci_unselectColor" format="color" />
        <!-- color of the stroke used to draw the indicators. -->
        <attr name="rci_strokeColor" format="color" />
        <!-- Whether or not the selected indicator snaps to the indicators. -->
        <attr name="rci_isSnap" format="boolean" />
    </declare-styleable>

    <declare-styleable name="OnBoardingView">

        <attr name="showPreNextView" format="boolean" />

        <attr name="colorChangeRequired" format="boolean" />

        <attr name="pageTransformer" format="enum">
            <enum name="alpha" value="0" />
            <enum name="cubein" value="1" />
            <enum name="cubeout" value="2" />
            <enum name="depthpage" value="3" />
            <enum name="fliphorizontal" value="4" />
            <enum name="flipvertical" value="5" />
            <enum name="rotationpage" value="6" />
            <enum name="zoomin" value="7" />
            <enum name="zoomout" value="8" />
            <enum name="zoomslide" value="9" />
            <enum name="temp" value="10" />
        </attr>

        <attr name="imagesForOnboarding" format="reference" />

        <attr name="titlesForOnboarding" format="reference" />

        <attr name="messagesForOnboarding" format="reference" />

        <attr name="colorsForOnboarding" format="reference" />

    </declare-styleable>
    <declare-styleable name="CustomFontTextView">
        <attr name="font" format="string" />
    </declare-styleable>


    <declare-styleable name="SwipeStack">
        <attr name="allowed_swipe_directions" format="integer">
            <enum name="only_left" value="1" />
            <enum name="only_right" value="2" />
            <enum name="both" value="0" />
        </attr>
        <attr name="animation_duration" format="integer" />
        <attr name="stack_size" format="integer" />
        <attr name="stack_spacing" format="dimension" />
        <attr name="stack_rotation" format="integer" />
        <attr name="swipe_rotation" format="float" />
        <attr name="swipe_opacity" format="float" />
        <attr name="scale_factor" format="float" />
        <attr name="disable_hw_acceleration" format="boolean" />
    </declare-styleable>
</resources>